def main():
    os.system('cls' if os.name == 'nt' else 'clear')
    
    print(Fore.MAGENTA + Style.BRIGHT + "‚îè‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îì")
    print(Fore.MAGENTA + Style.BRIGHT + "‚îÉ" + " " * 20 + Fore.YELLOW + Style.BRIGHT + "TOOL TEST CHO AXICU " + " " * 18 + Fore.MAGENTA + Style.BRIGHT + "‚îÉ")
    print(Fore.MAGENTA + Style.BRIGHT + "‚îó‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îõ")
    print()
    
    input_file = 'accounts.txt'
    xyz = []
    try:
        with open(input_file, 'r', encoding='utf-8') as file:
            accounts = [line for line in file if line.strip()]
        print(Fore.CYAN + Style.BRIGHT + f"ƒê√£ ƒë·ªçc {len(accounts)} t√†i kho·∫£n t·ª´ file {input_file}:")
        for i, line in enumerate(accounts, 1):
            username = line.split(':', 1)[0]
            print(Fore.GREEN + Style.BRIGHT + f"D√≤ng {i}: T√†i kho·∫£n {username}")
            xyz.append(username)
        print()
    except FileNotFoundError:
        print(Fore.RED + Style.BRIGHT + f"L·ªói: Kh√¥ng t√¨m th·∫•y file {input_file}")
        return


    with ThreadPoolExecutor(max_workers=5) as executor:
        futures = [
            executor.submit(process, i, line, accounts, xyz) for i, line in enumerate(accounts)
        ]
        for future in futures:
            future.result()
    print(Fore.GREEN + Style.BRIGHT + "\nHo√†n t·∫•t x·ª≠ l√Ω t·∫•t c·∫£ t√†i kho·∫£n!")
if __name__ == '__main__':
    main()
